{
    "name": "lshorz/lavatar",
    "description": "Create awesome unique avatar for Laravel",
    "keywords": ["Laravel", "avatar", "identicon", "image"],
    "authors": [
        {
            "name": "Marven Liao",
            "email": "lshzhuhai@163.com"
        }
    ],
    "license":"MIT",
    "require": {
        "php" : ">=7.0",
        "yzalis/identicon" : "~1.2"
    },
    "autoload" : {
        "psr-4" : {
            "Lshorz\\Lavatar\\" : "src/"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
	"extra": {
        "laravel": {
            "providers": [
                "Lshorz\\Lavatar\\LavatarServiceProvider"
            ],
            "aliases": {
                "Lavatar": "Lshorz\\Lavatar\\Facades\\Lavatar"
            }
        }
    }
}
